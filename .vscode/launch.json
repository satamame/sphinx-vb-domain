{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "shpinx_vb_domain",
            "type": "debugpy",
            "request": "launch",
            "program": ".venv/Scripts/sphinx-build.exe",
            "args": [
                "-b",
                "html",
                "docs/source",
                "docs/build",
                "-T",
                "-a",
                "-E",
                "-D",
                "vb_autodoc=1"
            ],
            "console": "integratedTerminal",
            "justMyCode": false
         }
    ]
  }
